<tr id="<<%= dom_id consume_history %>">
  <td><%= consume_history.consume_on %></td>

  <td><%= consume_history.quantity %></td>

  <td>
    <%= link_to edit_consume_history_path(consume_history) do %>
      <i class="fa-solid fa-pen"></i>
    <% end %>
  </td>

  <td>
    <%= link_to consume_history_path(consume_history), data: { turbo_method: :delete } do %>
      <i class="fa-solid fa-xmark"></i>
    <% end %>
  </td>
</tr>
